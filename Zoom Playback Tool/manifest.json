{
  "manifest_version": 3,
  "name": "Zoom Playback Speed Tool",
  "version": "1.0",
  "default_locale": "en",
  "description": "Enhance your Zoom experience with playback speed controls and keyboard shortcuts.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "host_permissions": [
    "*://*.zoom.us/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.zoom.us/rec/play/*"],
      "js": ["content.js"]
    }
  ]
}